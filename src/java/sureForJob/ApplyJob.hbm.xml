<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE   hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="sureForJob">
    <class name="ApplyJob" table="JobAcceptTable">
      <id name="id" column="id"/>
    <property name="userName" column="userName"/>
    <property name="email" column="email"/>
    <property name="contact" column="contact"/>
    <property name="address" column="address"/>
    <property name="photo" column="photo"/>
    <property name="percentage_10" column="percentage_10"/>
    <property name="percentage_12" column="percentage_12"/>
    <property name="cgpa" column="cgpa"/>
    <property name="Qualification" column="Qualification"/>
    <property name="confirm" column="confirm"/>
     
    </class>
</hibernate-mapping>